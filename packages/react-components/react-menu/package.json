{
  "name": "@fluentui/react-menu",
  "version": "9.12.15",
  "description": "Fluent UI menu component",
  "main": "lib-commonjs/index.js",
  "module": "lib/index.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/fluentui"
  },
  "license": "MIT",
  "scripts": {
    "build": "just-scripts build",
    "bundle-size": "monosize measure",
    "clean": "just-scripts clean",
    "code-style": "just-scripts code-style",
    "e2e": "cypress run --component",
    "e2e:local": "cypress open --component",
    "just": "just-scripts",
    "lint": "just-scripts lint",
    "start": "yarn storybook",
    "storybook": "start-storybook",
    "test": "jest --passWithNoTests",
    "type-check": "tsc -b tsconfig.json",
    "generate-api": "just-scripts generate-api",
    "test-ssr": "test-ssr \"./stories/**/*.stories.tsx\""
  },
  "devDependencies": {
    "@fluentui/eslint-plugin": "workspace:*",
    "@fluentui/react-conformance": "workspace:*",
    "@fluentui/react-conformance-griffel": "workspace:*",
    "@fluentui/scripts-api-extractor": "workspace:*",
    "@fluentui/scripts-cypress": "workspace:*",
    "@fluentui/scripts-tasks": "workspace:*"
  },
  "dependencies": {
    "@fluentui/keyboard-keys": "^9.0.4",
    "@fluentui/react-aria": "workspace:*",
    "@fluentui/react-context-selector": "workspace:*",
    "@fluentui/react-icons": "^2.0.207",
    "@fluentui/react-portal": "workspace:*",
    "@fluentui/react-positioning": "workspace:*",
    "@fluentui/react-shared-contexts": "workspace:*",
    "@fluentui/react-tabster": "workspace:*",
    "@fluentui/react-theme": "workspace:*",
    "@fluentui/react-utilities": "workspace:*",
    "@fluentui/react-jsx-runtime": "workspace:*",
    "@griffel/react": "^1.5.14",
    "@swc/helpers": "^0.5.1"
  },
  "peerDependencies": {
    "@types/react": ">=16.14.0 <19.0.0",
    "@types/react-dom": ">=16.14.0 <19.0.0",
    "react": ">=16.14.0 <19.0.0",
    "react-dom": ">=16.14.0 <19.0.0",
    "scheduler": "^0.19.0 || ^0.20.0"
  },
  "beachball": {
    "disallowedChangeTypes": [
      "major",
      "prerelease"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./lib-commonjs/index.js",
      "import": "./lib/index.js",
      "require": "./lib-commonjs/index.js"
    },
    "./package.json": "./package.json"
  }
}
