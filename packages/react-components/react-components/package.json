    "@fluentui/react-infobutton": "workspace:*",
    "@fluentui/react-input": "workspace:*",
    "@fluentui/react-label": "workspace:*",
    "@fluentui/react-link": "workspace:*",
    "@fluentui/react-menu": "workspace:*",
    "@fluentui/react-overflow": "workspace:*",
    "@fluentui/react-persona": "workspace:*",
    "@fluentui/react-portal": "workspace:*",
    "@fluentui/react-popover": "workspace:*",
    "@fluentui/react-positioning": "workspace:*",
    "@fluentui/react-progress": "workspace:*",
    "@fluentui/react-provider": "workspace:*",
    "@fluentui/react-radio": "workspace:*",
    "@fluentui/react-select": "workspace:*",
    "@fluentui/react-shared-contexts": "workspace:*",
    "@fluentui/react-skeleton": "workspace:*",
    "@fluentui/react-slider": "workspace:*",
    "@fluentui/react-spinbutton": "workspace:*",
    "@fluentui/react-spinner": "workspace:*",
    "@fluentui/react-switch": "workspace:*",
    "@fluentui/react-table": "workspace:*",
    "@fluentui/react-tabs": "workspace:*",
    "@fluentui/react-tabster": "workspace:*",
    "@fluentui/react-textarea": "workspace:*",
    "@fluentui/react-theme": "workspace:*",
    "@fluentui/react-toast": "workspace:*",
    "@fluentui/react-toolbar": "workspace:*",
    "@fluentui/react-tooltip": "workspace:*",
    "@fluentui/react-utilities": "workspace:*",
    "@fluentui/react-text": "workspace:*",
    "@fluentui/react-virtualizer": "workspace:*",
    "@fluentui/react-tree": "workspace:*",
