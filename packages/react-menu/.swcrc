{
  "env": {
    "coreJs": 3,
    "targets": "last 1 version, not IE 11, not dead",
    "mode": "entry"
  },
  "module": {
    "type": "es6"
  },
  "minify": true,
  "jsc": {
    "target": "es2019",
    "loose": true,
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "classic"
      },
      "optimizer": {
        "globals": {
          "vars": {
            "process.env.NODE_ENV": "production"
          }
        }
      }
    }
  }
}
